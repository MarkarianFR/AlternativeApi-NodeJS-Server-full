<script src="/socket.io/socket.io.js">
</script>

<script>
    var socket = io();

    socket.on('message', function(msg) {

        Messenger.options = {
            extraClasses: "messenger-fixed messenger-on-bottom  messenger-on-right",
            theme: "flat"
        }

        Messenger().post({
            message: msg.msg,
            type: msg.type
        })

    });
</script>

